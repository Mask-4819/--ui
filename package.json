{
  "name": "myapp",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "serve": "npm run build:theme && vite",
    "build": "run-p type-check build-only",
    "build:theme": "cd packages/theme-style && gulp build && cd..",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress run --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "new": "ts-node build/bin/new.ts",
    "new:js": "node build/bin/new.js",
    "scss": "gulp --gulpfile packages\\theme-style\\gulpfile.js"
  },
  "dependencies": {
    "jsoneditor": "^9.9.0",
    "pinia": "^2.0.14",
    "vue": "^3.2.36",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^18.0.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^10.0.2",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.0.0",
    "file-save": "^0.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^5.1.0",
    "jsdom": "^19.0.0",
    "npm-run-all": "^4.1.5",
    "ora": "^6.1.0",
    "prettier": "^2.5.1",
    "sass": "^1.53.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.7.2",
    "uppercamelcase": "^3.0.0",
    "vite": "^2.9.9",
    "vite-plugin-require": "^1.0.0",
    "vite-plugin-require-transform": "^1.0.3",
    "vitest": "^0.13.0",
    "vue-tsc": "^0.35.2"
  }
}
